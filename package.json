{
  "scripts": {
    "test": "cypress run",
    "merge-reports": "npx mochawesome-merge cypress/reports/*.json > cypress/reports/output.json",
    "generate-report": "npx marge cypress/reports/output.json --reportDir cypress/reports/html",
    "test:report": "npm run test && npm run merge-reports && npm run generate-report"
  },
  "devDependencies": {
    "cypress": "^15.3.0",
    "cypress-downloadfile": "^1.2.4",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0"
  },
  "dependencies": {
    "cypress-file-upload": "^5.0.8",
    "dayjs": "^1.11.18"
  }
}